package test;

public class Test {
	
	
	
public void test1() {
        // Anonymous Runnable
        Runnable r1 = new Runnable() {
                       @Override
                        public void run() {
                                        System.out.println("Not using Lambda");
                          }
         };
         r1.run();
}


public void test2() {
       // Lambda Runnable
       Runnable r1 = () -> System.out.println( "Not using Lambda");
       r1.run();
}


	 public static void main(String[] args) {
		 Runnable r1 = new Runnable() {
         @Override
          public void run() {
                          System.out.println("Not using Lambda");
           }
		};
		r1.run();
		
		Runnable r2 = () -> System.out.println( "Not using Lambda");
	       r1.run();
		 
	 }
}
